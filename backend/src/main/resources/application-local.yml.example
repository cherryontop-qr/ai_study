# application-local.yml.example
# 这是一个示例配置文件，请复制为 application-local.yml 并填入你的真实配置
# 注意：application-local.yml 已在 .gitignore 中，不会被提交到 Git

# 数据库配置（本地开发用）
spring:
  datasource:
    url: jdbc:mysql://localhost:3306/aistudy?useUnicode=true&characterEncoding=utf8&useSSL=false&serverTimezone=Asia/Shanghai
    username: root
    password: your_password_here

# DeepSeek AI 配置
deepseek:
  api-key: your_deepseek_api_key_here
  base-url: https://api.deepseek.com/chat/completions
  model: deepseek-chat

